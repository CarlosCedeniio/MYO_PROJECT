							 while (!(status & ALTERA_AVALON_UART_STATUS_RRDY_MSK))
							status=IORD_ALTERA_AVALON_UART_STATUS(UART_0_BASE);
							a= IORD_ALTERA_AVALON_UART_RXDATA(UART_0_BASE);
							status=ALTERA_AVALON_UART_STATUS_TRDY_MSK;
						}
						cont++;
					}
